---
permalink: /events.ics
layout: null
domain: heronwood.org
organizer:
  name: Heronwood Estates HOA Board
  mailto: board@heronwood.org
duration: PT1H
dateFormat: '%Y%m%dT%H%M%S'
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//hacksw/handcal//NONSGML v1.0//EN
{% for event in site.events reversed -%}
BEGIN:VEVENT
UID:{{ event.url }}@{{ page.domain }}
DTSTAMP:{{ event.date | date: page.dateFormat }}
DTSTAMP;TZID=US-Pacific:{{ event.date | date: page.dateFormat }}
ORGANIZER;CN={{ page.organizer.name }}:MAILTO:{{ page.organizer.mailto }}
DTSTART:{{ event.date | date: page.dateFormat }}
DTSTART;TZID=US-Pacific:{{ event.date | date: page.dateFormat }}
DURATION:{{ page.duration }}
SUMMARY:{{ event.title }}
LOCATION:{{ event.location }}
URL:{{ site.url }}{{ event.url }}
{% if event.description -%}
DESCRIPTION:{{ event.description }}
{% endif -%}
END:VEVENT
{% endfor -%}
END:VCALENDAR
